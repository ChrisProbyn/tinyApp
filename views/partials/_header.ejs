<% if(username) { %>
  <h1> User: <%= username %> </h1>
  <form action="/logout" method="POST" >
    <input type="submit" value="Logout">
  </form>
<% } %>
<% if(!username) { %>
  <form action="/login" method="POST" >
      <label for="longURL">Name:</label>
      <input id="username" type="text" name="username" placeholder="username" style="width: 300px">
      <input type="submit" value="Login">
  </form>
<% } %>